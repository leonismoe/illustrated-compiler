{
  "host": "//cdn.bootcss.com",
  "prefix": "/{name}/{version}/",
  "default": "{name}.{ext}",
  "libraries": {
    "normalize": "normalize.min.css",
    "font-awesome": "css/font-awesome.min.css",
    "animate.css": "animate.min.css",
